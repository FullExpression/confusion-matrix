{
    "name": "confusion-matrix",
    "version": "3.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "npm run generate-documentation && ng build confusion-matrix --prod && ng build web-app && gulp",
        "test": "ng test",
        "cm-build-test": "ng test --no-watch --project=confusion-matrix --browsers=ChromeHeadlessCI",
        "cm-code-coverage": "ng test --no-watch --project=confusion-matrix --code-coverage",
        "prod-deploy": "firebase deploy --only hosting:prod-web-app && firebase deploy --only hosting:prod-doc",
        "rc-deploy": "firebase deploy --only hosting:rc-web-app && firebase deploy --only hosting:rc-doc",
        "dev-deploy": "firebase deploy --only hosting:dev-web-app",
        "generate-documentation": "npm run generate-documentation --prefix projects/confusion-matrix",
        "lint": "ng lint",
        "publish": "npm run build && npm publish ./dist/confusion-matrix",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "@angular/animations": "~11.0.1",
        "@angular/cdk": "^11.0.3",
        "@angular/common": "~11.0.1",
        "@angular/compiler": "~11.0.1",
        "@angular/core": "~11.0.1",
        "@angular/forms": "~11.0.1",
        "@angular/material": "^11.0.3",
        "@angular/platform-browser": "~11.0.1",
        "@angular/platform-browser-dynamic": "~11.0.1",
        "@angular/router": "~11.0.1",
        "@fullexpression/confusion-matrix-stats": "^1.5.2",
        "angular2-draggable": "^2.3.2",
        "gulp": "^4.0.2",
        "html2canvas": "^1.0.0-rc.7",
        "ngx-color-picker": "^11.0.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.3",
        "@angular/cli": "~11.0.2",
        "@angular/compiler-cli": "~11.0.1",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.4",
        "ng-packagr": "^11.0.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}